{
  "eval_set_id": "roster_solver_eval",
  "name": "RosterSolver Agent Evaluation Set",
  "description": "Evaluates the RosterSolver agent for correct roster generation, failure handling, and overlap management",
  "metrics": [
    {
      "metric_name": "tool_trajectory_avg_score",
      "threshold": 0.9
    },
    {
      "metric_name": "response_match_score",
      "threshold": 0.5
    }
  ],
  "test_file": "roster_generation.test.json",
  "agent_module": "agents.solver_agent",
  "agent_factory": "create_solver_agent",
  "categories": {
    "basic_generation": [
      "generate_default_roster",
      "generate_roster_specific_date",
      "generate_two_week_roster",
      "generate_roster_custom_period"
    ],
    "staffing_simulation": [
      "simulate_staffing_hire",
      "simulate_staffing_promote"
    ],
    "safety": [
      "no_code_generation"
    ]
  }
}
