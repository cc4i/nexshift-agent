{
  "eval_set_id": "presenter_eval",
  "name": "RosterPresenter Agent Evaluation Set",
  "description": "Evaluates the RosterPresenter agent for correct roster presentation, approval flow, and failure handling",
  "metrics": [
    {
      "metric_name": "tool_trajectory_avg_score",
      "threshold": 0.85
    },
    {
      "metric_name": "response_match_score",
      "threshold": 0.6
    }
  ],
  "test_file": "presentation.test.json",
  "agent_module": "agents.presenter",
  "agent_factory": "create_presenter_agent",
  "categories": {
    "presentation": [
      "present_successful_roster",
      "include_compliance_summary",
      "include_empathy_score",
      "highlight_concerns"
    ],
    "approval_flow": [
      "approve_roster",
      "reject_roster_with_reason"
    ],
    "failure_handling": [
      "handle_generation_failure"
    ],
    "roster_management": [
      "list_pending_rosters",
      "list_all_rosters",
      "delete_rejected_roster"
    ],
    "edge_cases": [
      "idempotent_save"
    ]
  }
}
