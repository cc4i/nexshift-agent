{
  "eval_set_id": "coordinator_eval",
  "name": "RosteringCoordinator Agent Evaluation Set",
  "description": "Evaluates the RosteringCoordinator agent for correct query routing, tool selection, and orchestration",
  "metrics": [
    {
      "metric_name": "tool_trajectory_avg_score",
      "threshold": 0.85
    },
    {
      "metric_name": "response_match_score",
      "threshold": 0.5
    }
  ],
  "test_file": "query_routing.test.json",
  "agent_module": "agents.coordinator",
  "agent_factory": "create_coordinator_agent",
  "categories": {
    "nurse_queries": [
      "list_senior_nurses",
      "list_junior_nurses",
      "list_fatigued_nurses",
      "list_icu_certified",
      "list_all_nurses",
      "get_nurse_preferences",
      "get_specific_nurse_info",
      "check_nurse_availability",
      "get_nurse_stats",
      "get_nurse_history"
    ],
    "shift_queries": [
      "get_upcoming_shifts",
      "get_staffing_summary",
      "get_shift_history",
      "get_regulations"
    ],
    "roster_management": [
      "list_pending_rosters",
      "list_all_rosters",
      "get_specific_roster",
      "get_rosters_by_date_range",
      "finalize_roster",
      "reject_roster"
    ],
    "hris_management": [
      "add_new_nurse",
      "promote_nurse",
      "update_certifications"
    ],
    "direct_validation": [
      "validate_roster_directly",
      "analyze_roster_fairness"
    ]
  }
}
