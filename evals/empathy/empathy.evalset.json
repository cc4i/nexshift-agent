{
  "eval_set_id": "empathy_advocate_eval",
  "name": "EmpathyAdvocate Agent Evaluation Set",
  "description": "Evaluates the EmpathyAdvocate agent for fairness review, burnout detection, and preference honoring",
  "metrics": [
    {
      "metric_name": "tool_trajectory_avg_score",
      "threshold": 0.9
    },
    {
      "metric_name": "response_match_score",
      "threshold": 0.6
    }
  ],
  "test_file": "empathy_review.test.json",
  "agent_module": "agents.empathy",
  "agent_factory": "create_empathy_agent",
  "categories": {
    "basic_review": [
      "analyze_specific_roster",
      "analyze_default_roster",
      "check_fairness"
    ],
    "detailed_analysis": [
      "detailed_nurse_review",
      "get_nurse_stats_context",
      "comprehensive_empathy_review"
    ],
    "specific_checks": [
      "check_fatigue_levels",
      "check_preference_violations",
      "check_weekend_distribution",
      "check_night_shifts"
    ],
    "error_handling": [
      "handle_generation_failure"
    ],
    "recommendations": [
      "get_specific_nurse_history",
      "provide_recommendations"
    ]
  }
}
