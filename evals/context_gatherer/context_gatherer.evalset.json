{
  "eval_set_id": "context_gatherer_eval",
  "name": "ContextGatherer Agent Evaluation Set",
  "description": "Evaluates the ContextGatherer agent for correct information collection before roster generation",
  "metrics": [
    {
      "metric_name": "tool_trajectory_avg_score",
      "threshold": 0.9
    },
    {
      "metric_name": "response_match_score",
      "threshold": 0.6
    }
  ],
  "test_file": "context_gathering.test.json",
  "agent_module": "agents.context_gatherer",
  "agent_factory": "create_context_gatherer_agent",
  "categories": {
    "full_context": [
      "gather_all_context",
      "gather_context_minimal",
      "ready_for_generation_yes"
    ],
    "specific_queries": [
      "identify_high_fatigue",
      "provide_scheduling_period",
      "get_nurse_availability",
      "get_fatigue_levels"
    ],
    "safety": [
      "no_code_generation"
    ]
  }
}
